{
    "name": "itunes-vscode",
    "displayName": "itunes-vscode",
    "description": "iTunes Player",
    "version": "0.1.0",
    "publisher": "PsykoSoldi3r",
    "engines": {
        "vscode": "^1.5.0"
    },
    "repository": {
        "url": "https://github.com/PsykoSoldi3r/vscode-itunes"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "itunes.pause",
                "title": "iTunes Pause"
            },{
                "command": "itunes.play",
                "title": "iTunes Play"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "applescript": "^1.0.0"
    }
}
