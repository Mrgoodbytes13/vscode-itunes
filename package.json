{
    "name": "itunes-vscode",
    "displayName": "itunes-vscode",
    "description": "Control iTunes from within Visual Studio Code!",
    "version": "0.7.0",
    "publisher": "PsykoSoldi3r",
    "icon":"images/icon.png",
    "engines": {
        "vscode": "^1.5.0"
    },
    "repository": {
        "url": "https://github.com/PsykoSoldi3r/vscode-itunes"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "itunes.pause",
                "title": "iTunes: Pause"
            },{
                "command": "itunes.play",
                "title": "iTunes: Play"
            },{
                "command": "itunes.nextTrack",
                "title":"iTunes: Next Track"
            },{
                "command": "itunes.previousTrack",
                "title":"iTunes: Previous Track"
            },{
                "command": "itunes.open",
                "title":"iTunes: Show"
            },{
                "command":"itunes.repeat.set.one",
                "title":"iTunes: Set Repeat - One"
            },{
                "command":"itunes.repeat.set.all",
                "title":"iTunes: Set Repeat - All"
            },{
                "command":"itunes.repeat.set.off",
                "title":"iTunes: Set Repeat - Off"
            }
        ],
        "configuration":[
            {
                "title": "",
                "type":"object",
                "properties": {
                    "itunes.updateInterval":{
                        "type":"number",
                        "default":5000,
                        "description": "Interval of iTunes status check. (High value, lower CPU usage)"
                    }
                }
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "applescript": "^1.0.0"
    }
}
